{
    "name": "stencil-app",
    "private": true,
    "version": "0.0.1",
    "description": "Stencil App Starter",
    "scripts": {
        "build": "stencil build",
        "start": "stencil build --dev --watch --serve",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "generate": "stencil generate",
        "format": "prettier \"**/*.{js,css,html,json,ts}\" --write",
        "serve:prod": "npx live-server www"
    },
    "devDependencies": {
        "@stencil/core": "^1.12.2",
        "@stencil/router": "^1.0.1",
        "@types/jest": "^25.2.1",
        "live-server": "^1.2.1",
        "puppeteer": "^3.0.1"
    },
    "license": "MIT"
}
